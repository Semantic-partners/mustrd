@prefix rdf:       <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh:        <http://www.w3.org/ns/shacl#> .
@prefix must:      <https://mustrd.com/model/> .
@prefix test-data: <https://semanticpartners.com/data/test/> .

test-data:anzo_select
    a          must:TestSpec ;
    must:hasGiven [a must:given;
                must:dataSource [ a must:FileDataSource ;
                must:file "test/anzotest/data/given.ttl"
    ]];

    must:hasWhen [a must:when;
                must:dataSource [ a must:anzoQueryBuilderDataSource ;
                    must:queryFolder "My Queries";
                    must:queryName "mustrd-select"
                ];
        must:queryType must:SelectSparql
    ];

    must:hasThen [a must:given;
                must:dataSource [ a must:FileDataSource ;
                must:file "test/anzotest/data/thenSuccess.csv"
    ]] .
