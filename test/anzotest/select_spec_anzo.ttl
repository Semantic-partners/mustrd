    must:tripleStoreConfig [a must:anzoConfig;
        must:anzoURL "cmac.anzo-solutions.com";
        must:anzoPort 8946;
        must:anzoUser "sysadmin"; 
        must:anzoPassword "";
        must:gqeURI "http://openanzo.org/GqeDatasource/bc5a9bc3-5ec1-4134-b6f8-293a270b767f";
        must:inputGraph "http://cambridgesemantics.com/Layer/61bb695630494a1192076e3491476657"
    ];

    must:hasGiven [a must:given;
                must:dataSource [ a must:FileDataSource ;
                must:file "C:/Users/aymer/git/mustrd/test/anzotest/data/given.ttl"
