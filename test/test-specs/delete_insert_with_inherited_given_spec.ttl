@prefix must:      <https://mustrd.com/model/> .
@prefix test-data: <https://semanticpartners.com/data/test/> .
@prefix rdf:       <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

test-data:a_complete_delete_insert_with_inherited_given_scenario
    a          must:TestSpec ;
    must:given [ a must:InheritedState ] ;

    must:when  [ a must:TextDataSource ;
                                   must:queryText  "delete {?s ?p ?o} insert { ?o ?p ?s } where {?s ?p ?o}" ;
                 must:queryType must:UpdateSparql  ; ] ;
    must:then  [ a must:EmptyGraphResult ] .
