@prefix rdf:       <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh:        <http://www.w3.org/ns/shacl#> .
@prefix must:      <https://mustrd.com/model/> .
@prefix test-data: <https://semanticpartners.com/data/test/> .


# A graph result is not valid for a select statement, it should be table.
test-data:a_complete_select_scenario_with_empty_graph_result
    a          must:TestSpec ;
    must:given [ a must:StatementsDataSource ;
                                   must:statements [ a rdf:Statement ;
                                                     rdf:subject   test-data:sub ;
                                                     rdf:predicate test-data:pred ;
                                                     rdf:object    test-data:obj ; ] ; ] ;
    must:when  [ a must:TextDataSource ;
                                   must:queryText  "select ?s ?p ?o where { ?s ?p ?o }" ;
                 must:queryType must:SelectSparql   ; ] ;
    must:then  [ a must:EmptyGraphResult  ] .
