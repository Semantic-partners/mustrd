@prefix rdf:       <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh:        <http://www.w3.org/ns/shacl#> .
@prefix must:      <https://mustrd.com/model/> .
@prefix test-data: <https://semanticpartners.com/data/test/> .


test-data:a_complete_select_scenario_given_file
    a          must:TestSpec ;
    must:given [ a must:FileDataSource ;
                                   must:file "test/data/given.ttl" ] ;
    must:when  [ a must:textDataSource ;
                                   must:text  "SELECT ?s ?p ?o WHERE { ?s ?p ?o }" ;
                 must:queryType must:SelectSparql ; ] ;
    must:then  [ a must:TableDataSource ;
                 must:rows [ must:row [ must:variable "s" ;
                                        must:binding  test-data:sub ; ],
                                      [ must:variable "p" ;
                                        must:binding  test-data:pred ; ],
                                      [ must:variable "o" ;
                                        must:binding  test-data:obj ; ] ;
               ] ; ] .
