@prefix must:      <https://mustrd.com/model/> .
@prefix test-data: <https://semanticpartners.com/data/test/> .
@prefix rdf:       <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

test-data:a_complete_insert_scenario
    a          must:TestSpec ;
    must:given [ a               must:StatementsDataset ;
                 must:statements [ a             rdf:Statement ;
                                   rdf:subject   test-data:sub ;
                                   rdf:predicate test-data:pred ;
                                   rdf:object    test-data:obj ; ] ; ] ;
    must:when  [ a          must:UpdateSparql ;
                 must:query "insert { ?o ?p ?s } where {?s ?p ?o}" ; ] ;
    must:then  [ a               must:StatementsDataset ;
                 must:statements [ a             rdf:Statement ;
                                   rdf:subject   test-data:sub ;
                                   rdf:predicate test-data:pred ;
                                   rdf:object    test-data:obj ; ] ,
                                 [ a             rdf:Statement ;
                                   rdf:subject   test-data:obj ;
                                   rdf:predicate test-data:pred ;
                                   rdf:object    test-data:sub ; ] ; ] .
