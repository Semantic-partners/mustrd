@prefix rdf:       <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh:        <http://www.w3.org/ns/shacl#> .
@prefix must:      <https://mustrd.com/model/> .
@prefix test-data: <https://semanticpartners.com/data/test/> .


test-data:a_complete_select_scenario_with_variables
    a          must:TestSpec ;
    must:given [ a must:StatementsDataSource ;
                                   must:hasStatement [ a             rdf:Statement ;
                                   rdf:subject   test-data:sub ;
                                   rdf:predicate test-data:pred ;
                                   rdf:object    test-data:obj ; ] ,
                                  [ a             rdf:Statement ;
                                   rdf:subject   test-data:sub ;
                                   rdf:predicate test-data:pred ;
                                   rdf:object    "hello world" ; ] ; ] ;
    must:when  [ a must:TextSparqlSource ;
                                   must:queryText  "select ?s ?p ?o where { ?s ?p ?o }" ;
                 must:hasBinding [ must:variable "o" ;
                                 must:boundValue  "hello world" ; ] ;
                 must:queryType must:SelectSparql   ; ] ;
    must:then  [ a must:TableDataSource ;
                 must:hasRow [ must:hasBinding[ must:variable "s" ;
                                        must:boundValue  test-data:sub ; ],
                                      [ must:variable "p" ;
                                        must:boundValue  test-data:pred ; ],
                                      [ must:variable "o" ;
                                        must:boundValue  "hello world" ; ] ;
               ] ; ] .
