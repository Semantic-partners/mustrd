@prefix must:      <https://mustrd.com/model/> .
@prefix test-data: <https://semanticpartners.com/data/test/> .
@prefix rdf:       <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

test-data:a_complete_construct_scenario_with_variables
    a          must:TestSpec ;
    must:given [ must:dataSource [ a must:StatementsDataSource ;
                                   must:statements [ a rdf:Statement ;
                                                     rdf:subject   test-data:sub ;
                                                     rdf:predicate test-data:pred ;
                                                     rdf:object    "hello world" ; ] ; ] ; ] ;
    must:when  [ must:dataSource [ a must:textDataSource ;
                                   must:text  "construct { ?s <https://semanticpartners.com/data/test/predicate> ?o } { ?s ?p ?o }" ;
                 must:bindings [ must:variable "o" ;
                      must:binding  "hello world" ; ] ,
                       [ must:variable "s" ;
                      must:binding  test-data:sub ; ]
                       ; ] ;
                 must:queryType must:ConstructSparql  ; ] ;
    must:then  [ must:dataSource [ a must:StatementsDataSource ;
                 must:statements [ a             rdf:Statement ;
                                   rdf:subject   test-data:sub ;
                                   rdf:predicate test-data:predicate ;
                                   rdf:object    "hello world" ; ] ; ] ; ] .
