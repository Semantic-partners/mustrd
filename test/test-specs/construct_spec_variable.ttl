@prefix must:      <https://mustrd.com/model/> .
@prefix test-data: <https://semanticpartners.com/data/test/> .
@prefix rdf:       <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

test-data:a_complete_construct_scenario_with_variables
    a          must:TestSpec ;
    must:given [ a               must:StatementsDataset ;
                 must:statements [ a             rdf:Statement ;
                                   rdf:subject   test-data:sub ;
                                   rdf:predicate test-data:pred ;
                                   rdf:object    "hello world" ; ] ; ] ;
    must:when  [ a          must:ConstructSparql ;
                 must:query "construct { ?s <https://semanticpartners.com/data/test/predicate> ?o } { ?s ?p ?o }" ;
                 must:bindings [ must:variable "o" ;
                      must:binding  "hello world" ; ] ;
] ;
    must:then  [ a               must:StatementsDataset ;
                 must:statements [ a             rdf:Statement ;
                                   rdf:subject   test-data:sub ;
                                   rdf:predicate test-data:predicate ;
                                   rdf:object    "hello world" ; ] ; ] .
