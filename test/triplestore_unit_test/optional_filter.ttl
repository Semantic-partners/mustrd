@prefix rdf:       <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh:        <http://www.w3.org/ns/shacl#> .
@prefix must:      <https://mustrd.com/model/> .
@prefix test-data: <https://semanticpartners.com/data/test/> .


test-data:optional_filter
    a          must:TestSpec ;

    must:given [a must:FileDataSource ;
                must:file "./test/triplestore_unit_test/given/optional_filter.ttl"
    ];

    must:when [ a must:FileDataSource ;
                must:file "./test/triplestore_unit_test/when/optional_filter.sparql";
                must:queryType must:SelectSparql
    ];

    must:then [ a must:FileDataSource ;
                must:file "./test/triplestore_unit_test/then/optional_filter.csv"
    ].

