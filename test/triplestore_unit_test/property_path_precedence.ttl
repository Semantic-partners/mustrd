@prefix rdf:       <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh:        <http://www.w3.org/ns/shacl#> .
@prefix must:      <https://mustrd.com/model/> .
@prefix test-data: <https://semanticpartners.com/data/test/> .


test-data:NegatedPropertySet_plus
    a          must:TestSpec ;

    must:given [must:dataSource [ a must:FileDataSource ;
                must:file "./test/triplestore_unit_test/given/property_path_precedence.ttl"
    ]];

    must:when [must:dataSource [ a must:FileDataSource ;
                must:file "./test/triplestore_unit_test/when/NegatedPropertySet_plus.sparql"
        ];
        must:queryType must:SelectSparql
    ];

    must:then [must:dataSource [ a must:FileDataSource ;
                must:file "./test/triplestore_unit_test/then/NegatedPropertySet_plus.csv"
    ]].

