@prefix rdf:       <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh:        <http://www.w3.org/ns/shacl#> .
@prefix must:      <https://mustrd.com/model/> .
@prefix test-data: <https://semanticpartners.com/data/test/> .

test-data:subselect_filter_exists
a          must:TestSpec ;

must:given [ a must:FileDataset ;
            must:file "./given/subselect.ttl"
];

must:when [ a must:FileSparqlSource  ;
            must:file "./when/subselect_filter_exists.sparql";
            must:queryType must:SelectSparql
];

must:then [ a must:FileDataset ;
            must:file "./then/subselect_filter_exists.csv"
].
