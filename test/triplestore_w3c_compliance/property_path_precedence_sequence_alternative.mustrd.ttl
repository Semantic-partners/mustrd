@prefix rdf:       <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh:        <http://www.w3.org/ns/shacl#> .
@prefix must:      <https://mustrd.com/model/> .
@prefix test-data: <https://semanticpartners.com/data/test/> .

test-data:property_path_precedence_sequence_alternative
    a          must:TestSpec ;

    must:given [ a must:FileDataset ;
                must:file "./given/property_path_precedence.ttl"
    ];

    must:when [ a must:FileSparqlSource  ;
                must:file "./when/property_path_precedence_sequence_alternative.sparql";
                must:queryType must:SelectSparql
    ];

    must:then [ a must:FileDataset ;
                must:file "./then/property_path_precedence_sequence_alternative.csv"
    ].