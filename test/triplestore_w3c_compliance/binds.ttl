@prefix rdf:       <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh:        <http://www.w3.org/ns/shacl#> .
@prefix must:      <https://mustrd.com/model/> .
@prefix test-data: <https://semanticpartners.com/data/test/> .


test-data:binds
    a          must:TestSpec ;

    must:given [a must:FileDataSource ;
                must:file "./test/triplestore_w3c_compliance/given/placeholder.ttl"
    ];

    must:when [ a must:FileDataSource ;
                must:file "./test/triplestore_w3c_compliance/when/binds.sparql";
                must:queryType must:SelectSparql
    ];

    must:then [a must:FileDataSource ;
                must:file "./test/triplestore_w3c_compliance/then/binds.csv"
    ].

