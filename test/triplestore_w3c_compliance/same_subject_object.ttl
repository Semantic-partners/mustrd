@prefix rdf:       <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh:        <http://www.w3.org/ns/shacl#> .
@prefix must:      <https://mustrd.com/model/> .
@prefix test-data: <https://semanticpartners.com/data/test/> .


test-data:same_subject_object
    a          must:TestSpec ;

    must:given [ a must:FileDataSource ;
                must:file "./test/triplestore_w3c_compliance/given/same_subject_object.ttl"
    ];

    must:when [ a must:FileDataSource ;
                must:file "./test/triplestore_w3c_compliance/when/same_subject_object.sparql";
        must:queryType must:SelectSparql
    ];

    must:then [ a must:FileDataSource ;
                must:file "./test/triplestore_w3c_compliance/then/same_subject_object.csv"
    ].

