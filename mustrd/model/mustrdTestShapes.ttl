@prefix rdf:       <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh:        <http://www.w3.org/ns/shacl#> .
@prefix xsd:       <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:      <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ex:        <http://www.example.org/#> .
@prefix owl:       <http://www.w3.org/2002/07/owl#> .
@prefix mustrdTest:      <https://mustrd.com/mustrdTest/> .
@prefix triplestore: <https://mustrd.com/triplestore/> .


must:MustrdTestShape
    a              sh:NodeShape ;
    sh:targetClass mustrdTest:MustrdTest ;
    sh:property    [ sh:path     mustrdTest:hasDataPath ;
                     sh:minCount 1 ;
                     sh:maxCount 1 ],
                    [ sh:path     mustrdTest:hasSpecPath ;
                     sh:minCount 1 ; 
                     sh:maxCount 1 ],
                    [ sh:path     mustrdTest:hasTestFunction ;
                     sh:minCount 1 ; 
                     sh:maxCount 1 ],
                    [ sh:path     mustrdTest:filterOnTripleStore ;
                     sh:class triplestore:TripleStore ] . 

